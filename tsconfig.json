{
	"compilerOptions": {
		"target": "es6",
		"moduleResolution": "node",
		"jsx": "react",
		"module": "commonjs",
		"skipLibCheck": true,
		"sourceMap": true,
		"outDir": "./dist",
		"lib": [
			"es2016",
			"esnext.asynciterable",
			"dom"
		],
		"types": [
			"jest"
		],
		"esModuleInterop": true
	},
	"include": [
		"generated/**/*",
		"./index.ts"
	],
	"exclude": [
		"node_modules",
		"**/*.spec.ts",
		"**/*.test.ts",
	]
}